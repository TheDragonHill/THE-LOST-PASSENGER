  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 3319129135920545641}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: 08937329b23a3ff439e4c56676a937dd, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  currentDialogue: 0
  events:
  - connectedWaypoint: 0
    endEvent: 0
    endDecisions: 0
    talks:
    - name: Kilian
      sentence: "Sir, wie erwartet ist an dieser Stelle ein Fluss, aber es ist leider
        absolut unm\xF6glich um ihn herum zu gehen, uns bleibt nicht anderes \xFCbrig,
        als ihn zu \xFCberqueren."
    - name: Kilian
      sentence: "Leider wissen wir nicht genau wie tief das Wasser ist, wir k\xF6nnten
        versuchen einfach durchzulaufen und riskieren das es an manchen Stellen zu
        tief ist oder wir versuchen eine provisorische Br\xFCcke zu bauen."
    saveDecision: 0
    decisions:
    - decisionText: "Wir werden versuchen durch das Wasser zu gehen, eine Br\xFCcke
        zu bauen w\xFCrde uns zu viel Zeit kosten."
      nextDecision: 1
      eventDecisions: []
      nextWaypoint: {fileID: 0}
    - decisionText: "Wir werden eine Br\xFCcke bauen. Wir d\xFCrfen niemanden riskieren,
        entweder schaffen es alle \xFCber den Fluss oder keiner."
      nextDecision: 2
      eventDecisions:
      - valueName: timeHour
        valueNumber: -1
      nextWaypoint: {fileID: 0}
  - connectedWaypoint: 0
    endEvent: 1
    endDecisions: 0
    talks:
    - name: ' '
      sentence: Als deine Leute vorsichtig durch den Fluss laufen, um auch sicherzugehen,
        dass er nicht zu tief ist. Merken Sie bald das er sehr flach ist und man
        ohne bedenken durchlaufen kann.
    - name: Kilian
      sentence: "Anscheinend gab es keinen Grund zur Besorgnis. Zum Gl\xFCck haben
        wir unsere Zeit nicht mit dem Bauen einer Br\xFCcke verschwendet."
    saveDecision: 0
    decisions: []
  - connectedWaypoint: 0
    endEvent: 1
    endDecisions: 0
    talks:
    - name: ' '
      sentence: "Als ihr eine ganze Weile damit verbringt die Br\xFCcken zu bauen,
        f\xE4llt auf einmal einer der Baumst\xE4mme ins Wasser und jeder bemerkt
        das der Fluss nicht sehr tief ist."
    - name: Kilian
      sentence: "Anscheinend war es reine Zeitverschwendung diese Br\xFCcken zu bauen.
        Die Leute werden nicht sehr zufrieden damit sein."
    saveDecision: 0
    decisions: []
  currentEventTrigger: 0
  eventManager: {fileID: 0}
